<% if (Object.keys(config.links).length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title">Link</h3>
    <div class="widget">
      <% for (var linkTitle in config.links) { %>
        <% if(config.links[linkTitle]) {%>
          <a class="nav-icon <%- linkTitle %>-icon social-link" href="<%= config.links[linkTitle].url %>" target="_new"
            title="<%- linkTitle.charAt(0).toUpperCase() + linkTitle.slice(1) %> (Personal)"></a>
        <% } %>
      <% } %>
      <div class="clear"></div>
    </div>
  </div>
<% } %>
<% if (Object.keys(config.social_links).length){ %>
  <div class="widget-wrap">
    <h4 class="widget-title">Repositories contributed to</h4>
    <div class="widget">
      <% for (var socialName in config.social_links) { %>
        <% if(config.social_links[socialName]) {%>
          <a class="nav-icon <%- socialName %>-icon social-link" href="<%= config.social_links[socialName].TMDer %>" target="_new"
            title="<%- linkTitle.charAt(0).toUpperCase() + linkTitle.slice(1) %> (TMDer)"></a>
        <% } %>
      <% } %>
      <div class="clear"></div>
    </div>
  </div>
<% } %>
